{
  "kind": "collectionType",
  "collectionName": "activity_types",
  "info": {
    "singularName": "activity-type",
    "pluralName": "activity-types",
    "displayName": "Activity_type"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Active",
        "Inactive"
      ]
    },
    "version": {
      "type": "integer"
    },
    "created_by_user": {
      "type": "biginteger"
    },
    "updated_by_user": {
      "type": "biginteger"
    },
    "ID": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::activity.activity",
      "mappedBy": "activity_type_id"
    }
  }
}
